<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/textView_warning"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView_competition_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="64dp"
        android:text="대회 장소 :"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/competition_time" />

    <TextView
        android:id="@+id/competition_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="대회 장소 불러오기"
        app:layout_constraintBottom_toBottomOf="@+id/textView_competition_location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toEndOf="@+id/textView_competition_location"
        app:layout_constraintTop_toTopOf="@+id/textView_competition_location" />

    <Button
        android:id="@+id/button_apply"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:text="신청하기"
        app:backgroundTint="#36DA90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <TextView
        android:id="@+id/textView_competition_userBirth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:textColor="@color/black"
        android:text="생년월일 :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_competition_userNickName" />

    <TextView
        android:id="@+id/textView_competition_userNickName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="닉네임 :"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <TextView
        android:id="@+id/textView_competition_userLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:textColor="@color/black"
        android:text="내 위치 :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_competition_userBirth" />

    <TextView
        android:id="@+id/textView_competition_userEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:textColor="@color/black"
        android:text="이메일 :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_competition_userLocation" />

    <TextView
        android:id="@+id/competition_userNickName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="닉네임 불러오기"
        app:layout_constraintBottom_toTopOf="@+id/textView_competition_userBirth"
        app:layout_constraintStart_toEndOf="@+id/textView_competition_userNickName"
        app:layout_constraintTop_toTopOf="@+id/textView_competition_userNickName"
        app:layout_constraintVertical_bias="0.042" />

    <TextView
        android:id="@+id/competiton_userBirth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="생년월일 불러오기"
        app:layout_constraintStart_toEndOf="@+id/textView_competition_userBirth"
        app:layout_constraintTop_toTopOf="@+id/textView_competition_userBirth" />

    <TextView
        android:id="@+id/competition_userLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="위치 불러오기"
        app:layout_constraintStart_toEndOf="@+id/textView_competition_userLocation"
        app:layout_constraintTop_toTopOf="@+id/textView_competition_userLocation" />

    <TextView
        android:id="@+id/competition_userEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="이메일 불러오기"
        app:layout_constraintStart_toEndOf="@+id/textView_competition_userEmail"
        app:layout_constraintTop_toTopOf="@+id/textView_competition_userEmail" />

    <TextView
        android:id="@+id/competition_warningInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="신청 주의 사항 불러오기"
        app:layout_constraintBottom_toTopOf="@+id/button_apply"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.147"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_competition_location"
        app:layout_constraintVertical_bias="0.079" />

    <TextView
        android:id="@+id/textView_competition_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:text="신청 대회명 :"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_competition_userEmail" />

    <TextView
        android:id="@+id/textView_competition_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="28dp"
        android:text="대회 날짜 :"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_competition_title" />

    <TextView
        android:id="@+id/competition_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="대회명 불러오기"
        app:layout_constraintStart_toEndOf="@+id/textView_competition_title"
        app:layout_constraintTop_toTopOf="@+id/textView_competition_title" />



    <EditText
        android:id="@+id/competition_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.412"
        app:layout_constraintStart_toEndOf="@+id/textView_competition_time"
        app:layout_constraintTop_toTopOf="@+id/textView_competition_time"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>