<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/mainsearchappbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:fitsSystemWindows="true">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/searchviewtoolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            app:layout_collapseMode="pin"
            app:layout_scrollFlags="scroll|enterAlwaysCollapsed"
            app:title="@null"
            app:titleTextColor="@color/white">
            <androidx.cardview.widget.CardView
                android:id="@+id/searchviewcardview"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="15dp">
                <SearchView
                    android:id="@+id/searchviewsearch"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:iconifiedByDefault="false"
                    android:queryBackground="@null"
                    android:queryHint="검색어를 입력해주세요"
                    />
            </androidx.cardview.widget.CardView>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/searchresultswipe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <ScrollView
            android:id="@+id/searchviewscrollview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:nestedScrollingEnabled="true">
            <RelativeLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/searchviewtext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="최근 검색어"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="16sp"/>
                <Button
                    android:id="@+id/searchviewremovebtn"
                    android:layout_width="70dp"
                    android:layout_height="40dp"
                    android:textColor="@color/white"
                    android:background="@drawable/round_button"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="15dp"
                    android:textStyle="bold"
                    android:text="전체삭제"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/mainsearchrecyclerview"
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:layout_marginTop="10dp"
                    android:layout_below="@+id/searchviewtext"/>
                <TextView
                    android:id="@+id/timesearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="실시간 급상승 검색어"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_below="@id/mainsearchrecyclerview"/>
                <TextView
                    android:id="@+id/nowtime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="실시간 급상승 검색어"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginStart="10dp"
                    android:layout_alignParentRight="true"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_below="@id/mainsearchrecyclerview"/>
                <TextView
                    android:id="@+id/fisrtsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    android:layout_below="@+id/timesearch"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/firstsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/timesearch"
                    android:layout_toRightOf="@+id/fisrtsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/secondsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2"
                    android:layout_below="@+id/fisrtsearch"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/secondsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/fisrtsearch"
                    android:layout_toRightOf="@+id/secondsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/thirdsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="3"
                    android:layout_below="@+id/secondsearch"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/thirdsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/secondsearch"
                    android:layout_toRightOf="@+id/thirdsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/fourthsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4"
                    android:layout_below="@+id/thirdsearch"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/fourthsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/thirdsearch"
                    android:layout_toRightOf="@+id/fourthsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/fifthsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="5"
                    android:layout_below="@+id/fourthsearch"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/fifthsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/fourthsearchitem"
                    android:layout_toRightOf="@+id/fifthsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/sixthsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="6"
                    android:layout_below="@+id/timesearch"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/sixthsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/timesearch"
                    android:layout_toRightOf="@+id/sixthsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/seventhsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="7"
                    android:layout_below="@+id/sixthsearch"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/seventhsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/sixthsearch"
                    android:layout_toRightOf="@+id/seventhsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/eigthsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="8"
                    android:layout_below="@+id/seventhsearch"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/eigthsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/seventhsearch"
                    android:layout_toRightOf="@+id/eigthsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/ninthsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9"
                    android:layout_below="@+id/eigthsearch"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/ninthsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/eigthsearchitem"
                    android:layout_toRightOf="@+id/ninthsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/tenthsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="10"
                    android:layout_below="@+id/ninthsearch"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/tenthsearchitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/ninthsearchitem"
                    android:layout_toRightOf="@+id/tenthsearch"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/popluarsearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="인기 검색어"
                    android:layout_marginTop="30dp"
                    android:layout_marginStart="10dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_below="@id/tenthsearchitem"/>
                <TextView
                    android:id="@+id/fisrtsearch2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    android:layout_below="@+id/popluarsearch"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/firstpopitem2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/popluarsearch"
                    android:layout_toRightOf="@+id/fisrtsearch2"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/secondsearch2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2"
                    android:layout_below="@+id/fisrtsearch2"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/secondpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/firstpopitem2"
                    android:layout_toRightOf="@+id/secondsearch2"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/secondsearch3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="3"
                    android:layout_below="@+id/secondsearch2"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/thirdpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/secondsearch2"
                    android:layout_toRightOf="@+id/secondsearch3"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/secondsearch4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4"
                    android:layout_below="@+id/thirdpopitem"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/fourpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/thirdpopitem"
                    android:layout_toRightOf="@+id/secondsearch4"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/secondsearch5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="5"
                    android:layout_below="@+id/secondsearch4"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/fifpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/secondsearch4"
                    android:layout_toRightOf="@+id/secondsearch5"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/sixthsearch2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="6"
                    android:layout_below="@+id/popluarsearch"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/sixthpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/popluarsearch"
                    android:layout_toRightOf="@+id/sixthsearch2"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/seventhsearch2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="7"
                    android:layout_below="@+id/sixthsearch2"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/sevenpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/sixthsearch2"
                    android:layout_toRightOf="@+id/seventhsearch2"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/eigthsearch2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="8"
                    android:layout_below="@+id/seventhsearch2"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/eightpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/seventhsearch2"
                    android:layout_toRightOf="@+id/eigthsearch2"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/ninesearch2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9"
                    android:layout_below="@+id/eigthsearch2"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/ninepopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/eigthsearch2"
                    android:layout_toRightOf="@+id/ninesearch2"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:id="@+id/tensearch2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="10"
                    android:layout_below="@+id/ninesearch2"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/tenpopitem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:textColor="@color/black"
                    android:layout_below="@+id/ninesearch2"
                    android:layout_toRightOf="@+id/tensearch2"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="10dp"/>
            </RelativeLayout>
        </ScrollView>
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>