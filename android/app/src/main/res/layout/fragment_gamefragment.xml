<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.2" />
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline47"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.4" />
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline48"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.6" />
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline49"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.8" />
            <CheckBox
                android:id="@+id/aoscheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/_checkbox_aos"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline46"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:id="@+id/txt18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="AOS"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/aoscheckbox"
                app:layout_constraintStart_toStartOf="@+id/aoscheckbox"
                app:layout_constraintTop_toBottomOf="@+id/aoscheckbox" />
            <CheckBox
                android:id="@+id/rpgcheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/_checkbox_rpg"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline47"
                app:layout_constraintStart_toStartOf="@+id/guideline46"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:id="@+id/txt19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="RPG"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/rpgcheckbox"
                app:layout_constraintStart_toStartOf="@+id/rpgcheckbox"
                app:layout_constraintTop_toBottomOf="@+id/rpgcheckbox" />
            <CheckBox
                android:id="@+id/fpscheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/_checkbox_fps"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline48"
                app:layout_constraintStart_toStartOf="@+id/guideline47"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:id="@+id/txt20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FPS"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/fpscheckbox"
                app:layout_constraintStart_toStartOf="@+id/fpscheckbox"
                app:layout_constraintTop_toBottomOf="@+id/fpscheckbox" />
            <CheckBox
                android:id="@+id/cardgamecheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/_checkbox_cardgame"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline49"
                app:layout_constraintStart_toStartOf="@+id/guideline48"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:id="@+id/txt21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="카드게임"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/cardgamecheckbox"
                app:layout_constraintStart_toStartOf="@+id/cardgamecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/cardgamecheckbox" />
            <CheckBox
                android:id="@+id/braingamecheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/_checkbox_braingame"
                android:button="@null"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline49"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:id="@+id/txt22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="두뇌심리"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/braingamecheckbox"
                app:layout_constraintStart_toStartOf="@+id/braingamecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/braingamecheckbox" />
            <CheckBox
                android:id="@+id/sportgamecheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_sportgame"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline46"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt18" />
            <TextView
                android:id="@+id/txt23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="스포츠게임"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/sportgamecheckbox"
                app:layout_constraintStart_toStartOf="@+id/sportgamecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/sportgamecheckbox" />
            <CheckBox
                android:id="@+id/racinggamecheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_racinggame"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline47"
                app:layout_constraintStart_toStartOf="@+id/guideline46"
                app:layout_constraintTop_toBottomOf="@+id/txt18" />
            <TextView
                android:id="@+id/txt24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="레이싱게임"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/racinggamecheckbox"
                app:layout_constraintStart_toStartOf="@+id/racinggamecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/racinggamecheckbox" />
            <CheckBox
                android:id="@+id/nintendocheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_nintendo"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline48"
                app:layout_constraintStart_toStartOf="@+id/guideline47"
                app:layout_constraintTop_toBottomOf="@+id/txt18" />
            <TextView
                android:id="@+id/txt25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="닌텐도|플스"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/nintendocheckbox"
                app:layout_constraintStart_toStartOf="@+id/nintendocheckbox"
                app:layout_constraintTop_toBottomOf="@+id/nintendocheckbox" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="세부 카테고리 선택"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>