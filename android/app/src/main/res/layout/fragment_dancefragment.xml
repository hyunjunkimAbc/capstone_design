                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_hula"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline52"
                app:layout_constraintStart_toStartOf="@+id/guideline51"
                app:layout_constraintTop_toBottomOf="@+id/txt31" />
            <TextView
                android:id="@+id/txt38"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="발리댄스"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/hulacheckbox"
                app:layout_constraintStart_toStartOf="@+id/hulacheckbox"
                app:layout_constraintTop_toBottomOf="@+id/hulacheckbox" />
