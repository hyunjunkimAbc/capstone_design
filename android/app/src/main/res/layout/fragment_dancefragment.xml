                android:id="@+id/txt30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="힙합댄스"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/hiphopdancecheckbox"
                app:layout_constraintStart_toStartOf="@+id/hiphopdancecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/hiphopdancecheckbox" />
            <CheckBox
                android:id="@+id/housedancecheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_housedance"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline50"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt26" />
            <TextView
                android:id="@+id/txt31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="하우스"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/housedancecheckbox"
                app:layout_constraintStart_toStartOf="@+id/housedancecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/housedancecheckbox" />
            <CheckBox
                android:id="@+id/krumpcheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_krump"
