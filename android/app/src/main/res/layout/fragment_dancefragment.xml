                android:id="@+id/jazzdancecheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_jazzdance"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline53"
                app:layout_constraintStart_toStartOf="@+id/guideline52"
                app:layout_constraintTop_toBottomOf="@+id/txt31" />
            <TextView
                android:id="@+id/txt39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="재즈"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/jazzdancecheckbox"
                app:layout_constraintStart_toStartOf="@+id/jazzdancecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/jazzdancecheckbox" />
            <CheckBox
                android:id="@+id/aerobicscheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_aerobics"
                android:button="@null"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline53"
                app:layout_constraintTop_toBottomOf="@+id/txt31" />
            <TextView
                android:id="@+id/txt40"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="에어로빅"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/aerobicscheckbox"
                app:layout_constraintStart_toStartOf="@+id/aerobicscheckbox"
                app:layout_constraintTop_toBottomOf="@+id/aerobicscheckbox" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="세부 카테고리 선택"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>