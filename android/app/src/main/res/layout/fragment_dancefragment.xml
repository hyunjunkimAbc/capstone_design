            <CheckBox
                android:id="@+id/wacckingcheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/_checkbox_waccking"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline53"
                app:layout_constraintStart_toStartOf="@+id/guideline52"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:id="@+id/txt29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="왁킹"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/wacckingcheckbox"
                app:layout_constraintStart_toStartOf="@+id/wacckingcheckbox"
                app:layout_constraintTop_toBottomOf="@+id/wacckingcheckbox" />
            <CheckBox
                android:id="@+id/hiphopdancecheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
