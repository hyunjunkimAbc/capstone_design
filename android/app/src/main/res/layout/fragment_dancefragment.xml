                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/koreadancecheckbox"
                app:layout_constraintStart_toStartOf="@+id/koreadancecheckbox"
                app:layout_constraintTop_toBottomOf="@+id/koreadancecheckbox" />
            <CheckBox
                android:id="@+id/kpopcheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_kpop"
                android:button="@null"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline53"
                app:layout_constraintTop_toBottomOf="@+id/txt26" />
            <TextView
                android:id="@+id/txt35"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="K-POP"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/kpopcheckbox"
                app:layout_constraintStart_toStartOf="@+id/kpopcheckbox"
                app:layout_constraintTop_toBottomOf="@+id/kpopcheckbox" />
            <CheckBox
                android:id="@+id/valletcheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_vallet"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline50"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt31" />
            <TextView
                android:id="@+id/txt36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="발레"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/valletcheckbox"
                app:layout_constraintStart_toStartOf="@+id/valletcheckbox"
                app:layout_constraintTop_toBottomOf="@+id/valletcheckbox" />
            <CheckBox
                android:id="@+id/dancesportcheckbox"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/_checkbox_dancesport"
                android:button="@null"
                app:layout_constraintEnd_toStartOf="@+id/guideline51"
                app:layout_constraintStart_toStartOf="@+id/guideline50"
                app:layout_constraintTop_toBottomOf="@+id/txt31" />
            <TextView
                android:id="@+id/txt37"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="댄스스포츠"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/dancesportcheckbox"
                app:layout_constraintStart_toStartOf="@+id/dancesportcheckbox"
                app:layout_constraintTop_toBottomOf="@+id/dancesportcheckbox" />
            <CheckBox
                android:id="@+id/hulacheckbox"
                android:layout_width="50dp"
